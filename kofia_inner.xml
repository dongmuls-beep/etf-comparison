<?xml version="1.0" encoding="UTF-8"?><html xmlns="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:w2="http://www.inswave.com/websquare"	xmlns:xf="http://www.w3.org/2002/xforms">	<head>		<script src="/js/cond/srchCond_compSelect.js" type="javascript"/>	<!-- 회사선택/운용사선택 -->		<script src="/js/cond/srchCond_rptType.js" type="javascript"/>		<!-- 보고서유형 -->		<script src="/js/cond/srchCond_fundSearch.js" type="javascript"/>	<!-- 펀드검색 -->		<script src="/js/cond/srchCond_inquiryPeriod.js" type="javascript"/>	<!-- 조회기간 -->		<script src="/js/cond/srchCond_etc.js" type="javascript"/>	<!-- 수익률/기준 외 -->				<w2:buildDate />		<xf:model>			<xf:instance>				<data xmlns="" />			</xf:instance>		</xf:model>		<script type="javascript"><![CDATA[						$(function(){						initCond();		});		//조회조건 초기화		function initCond(){			MetaGridMaker.setGridData(vServiceId); //그리드 헤더 초기화			//setStdDtOp(0, 'RD');			//기준일 set			setStdDtOp(0, 'RD', '', manageInit);			fundTypeSet('','Y');			//펀드유형 셋팅 set			function manageInit() { //기준일 세팅된 후 운용사 콤보 구성				manageCompSelectBox(nextDate.getValue());		//운용사선택 selectbox set							}		};						function goSearch(){					var DISCondFuncDTO = {'DISCondFuncDTO' : {}};						var vEndDt = nextDate.getValue();		//기준일자 						var vCompCd = compCd.getValue();		//운용사선택			if(vCompCd == "all" || vCompCd == null || vCompCd =="" || vCompCd == undefined) vCompCd = "";			var uFundNm = fundNm.getValue(); 	//펀드명			var ufundTyp = fundTyp.getValue(); 		//펀드유형			if(ufundTyp == "all") ufundTyp="";						var ufundGb = fundGb.getValue(); 		//펀드종류			var uareaGb = areaGb.getValue(); 		//투자지역구분						CallProFrame.insertValue(DISCondFuncDTO, "tmpV30", vEndDt);		//기준일자			CallProFrame.insertValue(DISCondFuncDTO, "tmpV11", vCompCd);	//회사코드,(운용사선택)			CallProFrame.insertValue(DISCondFuncDTO, "tmpV12", uFundNm);	//펀드명				CallProFrame.insertValue(DISCondFuncDTO, "tmpV3", ufundTyp);	//펀드유형			CallProFrame.insertValue(DISCondFuncDTO, "tmpV5", ufundGb);	//펀드종류			CallProFrame.insertValue(DISCondFuncDTO, "tmpV4", uareaGb);		//투자지역구분				   		MetaGridMaker.setGridData(vServiceId, null, null, true, DISCondFuncDTO);   		}   		   		function reset(){   			groupCommon.init();   			   			setStdDtOp(0, 'RD');			//기준일 set   		}		        ]]></script>	</head>	<body>		<xf:group class="" id="group1" style="" tagname="form" name="searchForm">			<w2:attributes>				<w2:name></w2:name>				<w2:action>DISNewLawFund.xml</w2:action>				<w2:method>post</w2:method>			</w2:attributes>			<xf:group class="common_table_section" id="group2" style="">				<xf:input id="sendTsCd" name="sendTsCd" value="" type="hidden"></xf:input><!-- 보고서 선택리스트  -->				<xf:input id="cdList" name="cdList" value="" type="hidden"></xf:input><!-- 펀드 선택리스트  -->				<xf:input id="sendCompCd" name="sendCompCd" value="" type="hidden"></xf:input><!-- 회사 선택리스트 -->				<xf:input id="compOpGb" name="compOpGb" value="" type="hidden"></xf:input>				<xf:input id="uFundList" name="uFundList" value="" type="hidden"></xf:input>				<xf:group class="common_table1" id="groupCommon" style="" tagname="table">					<w2:attributes>						<w2:summary>펀드별 보수율과 TER를 운용사별,기준일자별,펀드명별,펀드유형별,펀드종류별,투자지역구분별로 제공합니다.</w2:summary>						<w2:cellspacing>0</w2:cellspacing>						<w2:collpadding>0</w2:collpadding>					</w2:attributes>					<w2:textbox id="textbox11z" label="펀드별 보수비용비교의 검색조건을 선택합니다." style="" tagname="caption"></w2:textbox>					<xf:group class="" id="group4" style="" tagname="colgroup">						<xf:group class="" id="group5" style="" tagname="col">							<w2:attributes>								<w2:width>126</w2:width>							</w2:attributes>						</xf:group>						<xf:group class="" id="group6" style="" tagname="col">							<w2:attributes>								<w2:width>250</w2:width>							</w2:attributes>						</xf:group>						<xf:group class="" id="group149" style="" tagname="col">							<w2:attributes>								<w2:width>126</w2:width>							</w2:attributes>						</xf:group>						<xf:group class="" id="group150" style="" tagname="col">							<w2:attributes>								<w2:width>*</w2:width>							</w2:attributes>						</xf:group>					</xf:group>					<!-- 운용사선택 -->					<xf:group class="" id="condition_group69" style="" tagname="tr">						<xf:group class="table1_th" id="condition_group70" style="" tagname="th">							<w2:textbox for="report_select_input_0" id="sendCompSelectedNm" label="운용사선택" style="" tagname="label"></w2:textbox>						</xf:group>						<xf:group class="table1_td" id="condition_group71" style="" tagname="td">							<w2:attributes>								<w2:colspan>3</w2:colspan>							</w2:attributes>							<xf:select1 allOption="true" appearance="minimal" chooseOption="" class="common_table_select" direction="auto"								disabled="false" disabledClass="w2selectbox_disabled" displayMode="label" id="compCd" renderType="native"								style="position:relative;width:440px;" submenuSize="auto" title="운용사 선택">								<xf:choices></xf:choices>							</xf:select1>						</xf:group>					</xf:group>										<!-- 기준일자 -->					<xf:group class="" id="condition_group72" style="" tagname="tr">						<xf:group class="table1_th" id="condition_group73" style="" tagname="th">							<w2:textbox for="report_select_input_0" id="condition_textbox28" label="기준일자" style="" tagname="label"></w2:textbox>						</xf:group>						<xf:group class="table1_td" id="condition_group74" style="" tagname="td">							<w2:attributes>								<w2:colspan>3</w2:colspan>							</w2:attributes>							<xf:select1 appearance="full" class="common_table_ipt_ul fl" cols="" id="stdDtOp" itemTagname="li"								renderType="radiogroup" rows="" selectedIndex="0" style="" tagname="ul">								<xf:choices>									<xf:item>										<xf:label>최신일 기준</xf:label>										<xf:value>RD</xf:value>									</xf:item>									<xf:item>										<xf:label>최근 월말</xf:label>										<xf:value>RM</xf:value>									</xf:item>									<xf:item>										<xf:label>최근 년도</xf:label>										<xf:value>RY</xf:value>									</xf:item>								</xf:choices>								<script type="javascript" ev:event="onradioclick(index,value)"><![CDATA[									setStdDtOp(index, value);								]]></script>																<script ev:event="onlabelclick(index,value)" type="javascript"><![CDATA[ 									setStdDtOp(index, value);								]]></script>							</xf:select1>							<w2:inputCalendar calendarImage="/img/com/ico_calendar.gif" title="기준일자 선택"								calendarImageOver="/img/com/ico_calendar.gif" class="common_calrendar" id="nextDate"								style="position: relative ;width: 100px;" weekStartsOn="0">							</w2:inputCalendar>						</xf:group>					</xf:group>					<!-- 펀드명 -->					<xf:group class="table1_th" id="condition_group82" style="" tagname="th">						<w2:textbox for="fundNm" id="condition_textbox31" label="펀드명" style="" tagname="label"></w2:textbox>					</xf:group>					<xf:group class="table1_td" id="condition_group83" style="" tagname="td">						<w2:attributes>							<w2:colspan>3</w2:colspan>						</w2:attributes>						<xf:input class="common_table_ipt" id="fundNm" style="position: relative ;width:440px;"></xf:input>						<script type="javascript" ev:event="onkeydown"><![CDATA[ 							if(event.keyCode == "13"){								if (goSearch){									goSearch();											}							}						]]></script>					</xf:group>															<!-- 펀드유형  -->					<xf:group class="" id="condition_group106" style="" tagname="tr">						<xf:group class="table1_th" id="condition_group107" style="" tagname="th">							<w2:textbox for="report_select_input_0" id="condition_textbox39" label="펀드유형" style="" tagname="label"></w2:textbox>						</xf:group>						<xf:group class="table1_td" id="condition_group108" style="" tagname="td">							<xf:select1 allOption="true" appearance="minimal" chooseOption="false" class="common_table_select"								direction="auto" disabled="false" disabledClass="w2selectbox_disabled" displayMode="label" id="fundTyp" title="펀드유형선택" renderType="native"								style="position:relative;width:195px;" submenuSize="auto" chooseOptionLabel="">								<xf:choices></xf:choices>							</xf:select1>						</xf:group>												<!-- 펀드종류 -->						<xf:group class="table1_th" id="condition_group85" style="" tagname="th">							<w2:textbox for="report_select_input_0" id="condition_textbox32" label="펀드종류" style="" tagname="p"></w2:textbox>						</xf:group>						<xf:group class="table1_td" id="condition_group86" style="" tagname="td">							<xf:select1 appearance="full" class="common_table_ipt_ul fl" cols="" id="fundGb" itemTagname="li"								renderType="radiogroup" rows="" selectedIndex="0" style="" tagname="ul">								<xf:choices>									<xf:item>										<xf:label>전체</xf:label>										<xf:value></xf:value>									</xf:item>									<xf:item>										<xf:label>투자신탁</xf:label>										<xf:value>5</xf:value>									</xf:item>									<xf:item>										<xf:label>투자회사</xf:label>										<xf:value>M</xf:value>									</xf:item>								</xf:choices>							</xf:select1>						</xf:group>					</xf:group>					<!-- 투자지역구분 -->					<xf:group class="" id="condition_group87" style="" tagname="tr">						<xf:group class="table1_th" id="condition_group88" style="" tagname="th">							<w2:textbox for="report_select_input_0" id="condition_textbox33" label="투자지역구분" style="" tagname="p"></w2:textbox>						</xf:group>						<xf:group class="table1_td" id="condition_group89" style="" tagname="td">							<w2:attributes>								<w2:colspan>3</w2:colspan>							</w2:attributes>							<xf:select1 appearance="full" class="common_table_ipt_ul fl" cols="" id="areaGb" itemTagname="li"								renderType="radiogroup" rows="" selectedIndex="0" style="" tagname="ul">								<xf:choices>									<xf:item>										<xf:label>전체</xf:label>										<xf:value></xf:value>									</xf:item>									<xf:item>										<xf:label>국내</xf:label>										<xf:value>1</xf:value>									</xf:item>									<xf:item>										<xf:label>해외</xf:label>										<xf:value>4</xf:value>									</xf:item>									<xf:item>										<xf:label>해외 30</xf:label>										<xf:value>3</xf:value>									</xf:item>									<xf:item>										<xf:label>해외 60</xf:label>										<xf:value>2</xf:value>									</xf:item>								</xf:choices>							</xf:select1>						</xf:group>					</xf:group>					<!-- 버튼 -->					<xf:group class="" id="DISFundFeeCMSBtnList" style="" tagname="tr">						<xf:group class="table1_td" id="group142" style="" tagname="td">							<w2:attributes>								<w2:colspan>4</w2:colspan>							</w2:attributes>							<xf:group class="right mr40" id="group143" style="">								<xf:group id="btnSear" style="" tagname="a" class="mr5">									<w2:attributes>										<w2:href>javascript:goSearch();</w2:href>									</w2:attributes>									<xf:image id="btnSearImg" src="/img/com/btn_common_table_sear.gif" style="" alt="검색"></xf:image>								</xf:group>								<xf:group class="mr5" id="btnReset" style="" tagname="a">									<w2:attributes>										<w2:href>javascript:reset();</w2:href>									</w2:attributes>									<xf:image id="btnResetImg" src="/img/com/btn_common_table_reset.gif" style="" alt="초기화"></xf:image>								</xf:group>							</xf:group>						</xf:group>					</xf:group>				</xf:group>				<xf:group class="sear_table_event" id="sear_table_event" style="">					<xf:group class="" id="group148" style="" tagname="a">						<w2:attributes>							<w2:href>#!</w2:href>							<w2:onclick>commonTableEvent(this);</w2:onclick>						</w2:attributes>						<xf:image id="sear_table_event_btn" src="/img/com/btn_table_hide.gif" style="" alt="조건검색 숨기기"></xf:image>					</xf:group>				</xf:group>			</xf:group>		</xf:group>					</body></html>